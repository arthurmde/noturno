{
  "manifest_version": 2,
  "name": "noturno",
  "version": "1.0.0",
  "description": "Easily switch between proxy configurations",
  "background": {
    "scripts": ["index.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/off_16.png",
      "32": "data/off_32.png",
      "64": "data/off_64.png"
    }
  }
}
